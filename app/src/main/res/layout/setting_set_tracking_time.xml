<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/setting_set_tracking_time_layout"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    style="@style/settingContainer">

    <TextView
        android:id="@+id/setting_set_tracking_time_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="to_be_set_in_fragment"
        style="@style/SettingTitle"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/setting_set_tracking_time_text"
        style="@style/SettingText"/>

</LinearLayout>